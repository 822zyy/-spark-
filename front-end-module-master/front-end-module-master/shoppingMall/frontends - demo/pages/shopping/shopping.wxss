.top{
  height: 210px;
  border: 2px solid #ddd;
  display: flex;
  padding:20px;
  box-sizing: border-box;
  justify-content: space-between;
  flex-wrap: wrap;
}
.top-item{
  width: 80px;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.top-item .text{
  font-size: 14px;
}
.top-item image{
  width: 50px;
  height: 50px;
}